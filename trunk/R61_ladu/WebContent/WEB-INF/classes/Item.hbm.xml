<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="Item.core.Item.Item" table="Item">
  <id column="Item" name="Item" type="java.lang.Integer">
   <generator class="increment"/>
  </id>
  <property generated="never" lazy="false" name="Item">
   <column name="Item"/>
  </property>
  <property generated="never" lazy="false" name="item_type_fk">
   <column name="item_type_fk"/>
  </property>
  <property generated="never" lazy="false" name="name">
   <column name="name"/>
  </property>
  <property generated="never" lazy="false" name="store_price">
   <column name="store_price"/>
  </property>
  <property generated="never" lazy="false" name="sale_price">
   <column name="sale_price"/>
  </property>
  <property generated="never" lazy="false" name="producer">
   <column name="producer"/>
  </property>
  <property generated="never" lazy="false" name="description">
   <column name="description"/>
  </property>
  <property generated="never" lazy="false" name="serial_no">
   <column name="serial_no"/>
  </property>
  <property generated="never" lazy="false" name="single_item">
   <column name="single_item"/>
  </property>
  <property generated="never" lazy="false" name="producer_code">
   <column name="producer_code"/>
  </property>
  <property generated="never" lazy="false" name="upper_item_fk">
   <column name="upper_item_fk"/>
  </property>
  <property generated="never" lazy="false" name="created">
   <column name="created"/>
  </property>
 </class>
</hibernate-mapping>
